{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "nevo/booking-calendar",
	"version": "1.0.0",
	"title": "NEVO Booking Calendar",
	"category": "nevo-blocks",
	"icon": "calendar-alt",
	"description": "Kalendarz rezerwacji konsultacji NEVO",
	"keywords": ["kalendarz", "calendar", "rezerwacja", "booking", "konsultacja"],
	"supports": {
		"html": false,
		"align": ["wide", "full"],
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"attributes": {
		"title": {
			"type": "string",
			"default": "Umów bezpłatną konsultację"
		},
		"subtitle": {
			"type": "string",
			"default": "30 minut rozmowy o Twoim biznesie. Bez zobowiązań."
		},
		"apiBase": {
			"type": "string",
			"default": "https://n8n.nevomarketing.pl/webhook"
		}
	},
	"textdomain": "nevo",
	"editorScript": "file:./index.js",
	"viewScript": "file:./view.js",
	"style": "file:./style-index.css",
	"editorStyle": "file:./index.css"
}
