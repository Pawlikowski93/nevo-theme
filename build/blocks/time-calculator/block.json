{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "nevo/time-calculator",
	"version": "1.0.0",
	"title": "NEVO Kalkulator Czasu",
	"category": "nevo-blocks",
	"icon": "clock",
	"description": "Interaktywny kalkulator oszczędności czasu",
	"keywords": ["kalkulator", "calculator", "czas", "time", "oszczędności"],
	"supports": {
		"html": false,
		"align": ["wide", "full"],
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"attributes": {
		"title": {
			"type": "string",
			"default": "Ile czasu tracisz na powtarzalne zadania?"
		},
		"subtitle": {
			"type": "string",
			"default": "Przesuń suwaki i zobacz, ile godzin możesz odzyskać"
		},
		"ctaText": {
			"type": "string",
			"default": "Odzyskaj swój czas"
		},
		"ctaUrl": {
			"type": "string",
			"default": "#kontakt"
		},
		"hourlyRate": {
			"type": "number",
			"default": 100
		}
	},
	"textdomain": "nevo",
	"editorScript": "file:./index.js",
	"viewScript": "file:./view.js",
	"style": "file:./style-index.css",
	"editorStyle": "file:./index.css"
}
