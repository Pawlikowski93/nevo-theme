{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "nevo/ai-chat",
	"version": "1.0.0",
	"title": "NEVO AI Chat",
	"category": "nevo-blocks",
	"icon": "format-chat",
	"description": "Widget czatu AI - wkrótce dostępny",
	"keywords": ["chat", "ai", "asystent", "bot", "n8n"],
	"textdomain": "nevo",
	"supports": {
		"align": ["wide"],
		"html": false,
		"anchor": true
	},
	"attributes": {
		"title": {
			"type": "string",
			"default": "Porozmawiaj z AI"
		},
		"welcomeMessage": {
			"type": "string",
			"default": "Cześć! Jestem asystentem NEVO. Jak mogę Ci pomóc?"
		},
		"badgeText": {
			"type": "string",
			"default": "Wkrótce dostępne"
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./editor.css",
	"style": "file:./style.css"
}
