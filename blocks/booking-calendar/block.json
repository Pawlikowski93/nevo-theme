{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "nevo/booking-calendar",
	"version": "1.0.0",
	"title": "NEVO Kalendarz Rezerwacji",
	"category": "nevo-blocks",
	"icon": "calendar-alt",
	"description": "Embed kalendarza Cal.com do rezerwacji spotkań",
	"keywords": ["kalendarz", "calendar", "rezerwacja", "booking", "cal.com", "spotkanie"],
	"textdomain": "nevo",
	"supports": {
		"align": ["wide"],
		"html": false,
		"anchor": true
	},
	"attributes": {
		"title": {
			"type": "string",
			"default": "Zarezerwuj rozmowę"
		},
		"calendarUrl": {
			"type": "string",
			"default": ""
		},
		"embedType": {
			"type": "string",
			"default": "inline"
		}
	},
	"editorScript": "file:./index.js",
	"editorStyle": "file:./editor.css",
	"style": "file:./style.css"
}
